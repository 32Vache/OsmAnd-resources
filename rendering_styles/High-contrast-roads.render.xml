<renderingStyle name="High-contrast-roads" depends="default" defaultColor="#f1eae4" version="1">

	<!--
		Depends on All-Purpose Renderer (by Hardy Mueller), based on production version 2012-01-04
	--> 

	<!--
		Try high contrast roads, suggested by Dirk Sommer: primary = #C00828, secondary = #FF8000, tertiary = #F0C810
		2012-10-07 Simplify using Constants
	--> 

	<!-- roadColors="High contrast roads" -->
	<renderingConstant name="null" value="#00000000"/>
	<renderingAttribute name="motorwayRoadColor">
		<case attrColorValue="#3838B0">
			<apply_if nightMode="true" attrColorValue="#3838B0"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="motorwayRoadLowZoomColor">
		<switch>
			<case publicTransportMode="true"/>
			<case tramTrainRoutes="true"/>
			<case subwayMode="true"/>
			<apply attrColorValue="#ffc7d0">
				<apply_if nightMode="true" attrColorValue="#866f64"/>
			</apply>
		</switch>
		<case attrColorValue="#3838B0">
			<apply_if nightMode="true" attrColorValue="#3838B0"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="motorwayRoadShadowColor">
		<switch>
			<case publicTransportMode="true"/>
			<case tramTrainRoutes="true"/>
			<case subwayMode="true"/>
			<apply attrColorValue="#bbbbbb">
				<apply_if nightMode="true" attrColorValue="#555555"/>
			</apply>
		</switch>
		<case attrColorValue="#969696">
			<apply_if nightMode="true" attrColorValue="$null"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="trunkRoadColor">
		<case attrColorValue="#A01060">
			<apply_if nightMode="true" attrColorValue="#A01060"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="trunkRoadLowZoomColor">
		<switch>
			<case publicTransportMode="true"/>
			<case tramTrainRoutes="true"/>
			<case subwayMode="true"/>
			<apply attrColorValue="#ffc7d0">
				<apply_if nightMode="true" attrColorValue="#866f64"/>
			</apply>
		</switch>
		<case attrColorValue="#A01060">
			<apply_if nightMode="true" attrColorValue="#A01060"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="trunkRoadShadowColor">
		<switch>
			<case publicTransportMode="true"/>
			<case tramTrainRoutes="true"/>
			<case subwayMode="true"/>
			<apply attrColorValue="#bbbbbb">
				<apply_if nightMode="true" attrColorValue="#555555"/>
			</apply>
		</switch>
		<case attrColorValue="#969696">
			<apply_if nightMode="true" attrColorValue="$null"/>
		</case>
	</renderingAttribute>
<!-- Primary  -->
	<renderingAttribute name="primaryRoadColor">
		<case attrColorValue="#C00828">
			<apply_if nightMode="true" attrColorValue="#C00828"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="primaryRoadLowZoomColor">
		<switch>
			<case publicTransportMode="true"/>
			<case tramTrainRoutes="true"/>
			<case subwayMode="true"/>
			<apply attrColorValue="#ffd9c7">
				<apply_if nightMode="true" attrColorValue="#866f64"/>
			</apply>
		</switch>
		<case attrColorValue="#C00828">
			<apply_if nightMode="true" attrColorValue="#C00828"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="primaryRoadShadowColor">
		<switch>
			<case publicTransportMode="true"/>
			<case tramTrainRoutes="true"/>
			<case subwayMode="true"/>
			<apply attrColorValue="#bbbbbb">
				<apply_if nightMode="true" attrColorValue="#555555"/>
			</apply>
		</switch>
		<case attrColorValue="#77464646"/>
	</renderingAttribute>
	<renderingAttribute name="secondaryRoadColor">
		<case attrColorValue="#FF8000">
			<apply_if nightMode="true" attrColorValue="#FF8000"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="secondaryRoadLowZoomColor">
		<switch>
			<case publicTransportMode="true"/>
			<case tramTrainRoutes="true"/>
			<case subwayMode="true"/>
			<apply attrColorValue="#ffe7bc">
				<apply_if nightMode="true" attrColorValue="#866f64"/>
			</apply>
		</switch>
		<case attrColorValue="#FF8000">
			<apply_if nightMode="true" attrColorValue="#FF8000"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="secondaryRoadShadowColor">
		<switch>
			<case publicTransportMode="true"/>
			<case tramTrainRoutes="true"/>
			<case subwayMode="true"/>
			<apply attrColorValue="#bbbbbb">
				<apply_if nightMode="true" attrColorValue="#555555"/>
			</apply>
		</switch>
		<case attrColorValue="#77464646"/>
	</renderingAttribute>
<!-- Tertiary  -->
	<renderingAttribute name="tertiaryRoadColor">
		<case attrColorValue="#F0C810">
			<apply_if nightMode="true" attrColorValue="#F0C810"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="tertiaryRoadLowZoomColor">
		<switch>
			<case publicTransportMode="true"/>
			<case tramTrainRoutes="true"/>
			<case subwayMode="true"/>
			<apply attrColorValue="#feffc1">
				<apply_if nightMode="true" attrColorValue="#5c5c4a"/>
			</apply>
		</switch>
		<case attrColorValue="#F0C810">
			<apply_if nightMode="true" attrColorValue="#F0C810"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="tertiaryRoadShadowColor">
		<case attrColorValue="#969696"/>
	</renderingAttribute>

	<renderingAttribute name="highwayMotorwayTextColor">
		<case attrColorValue="$white"/>
	</renderingAttribute>
	<renderingAttribute name="highwayTrunkTextColor">
		<case attrColorValue="$white"/>
	</renderingAttribute>
	<renderingAttribute name="highwayPrimaryTextColor">
		<case attrColorValue="$white"/>
	</renderingAttribute>

<!--		<renderingConstant name="motorwayRoadColor" value="#3838B0"/>
		<renderingConstant name="trunkRoadColor" value="#A01060"/>
		<renderingConstant name="primaryRoadColor" value="#C00828"/>
		<renderingConstant name="secondaryRoadColor" value="#FF8000"/>
		<renderingConstant name="tertiaryRoadColor" value="#F0C810"/>-->


	<order>
	</order>


	<text>
	</text>


	<point>
	</point>


	<polygon>
	</polygon>


	<line>
	</line>

</renderingStyle>
