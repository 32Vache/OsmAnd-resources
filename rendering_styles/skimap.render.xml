<renderingStyle name="Ski-map" depends="default" defaultColor="#f5ffff" version="1">

	<renderingAttribute name="defaultColor">
		<case noPolygons="true" attrColorValue="#00ffffff"/>
		<case nightMode="true" attrColorValue="#003333"/>
		<case attrColorValue="#f5ffff"/>
	</renderingAttribute>
	<renderingProperty attr="pisteGrooming" name="Piste grooming" description="Describes the style of piste preparation" type="boolean" possibleValues="" category="details"/>


<!-- Highways -->
	<renderingAttribute name="motorwayRoadColor">
		<case attrColorValue="#e6d9ff">
			<apply_if nightMode="true" attrColorValue="#384b54"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="motorwayRoadLowZoomColor">
		<case attrColorValue="#e6d9ff">
			<apply_if nightMode="true" attrColorValue="#384b54"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="motorwayRoadShadowColor">
		<case attrColorValue="#969696">
			<apply_if nightMode="true" attrColorValue="#55ffffff"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="trunkRoadColor">
		<case attrColorValue="#ffdee4">
			<apply_if nightMode="true" attrColorValue="#5f5556"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="trunkRoadLowZoomColor">
		<case attrColorValue="#ffdee4">
			<apply_if nightMode="true" attrColorValue="#5f5556"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="trunkRoadShadowColor">
		<case attrColorValue="#969696">
			<apply_if nightMode="true" attrColorValue="#55ffffff"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="primaryRoadColor">
		<case attrColorValue="#ffe9de">
			<apply_if nightMode="true" attrColorValue="#544b4c"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="primaryRoadLowZoomColor">
		<case attrColorValue="#ffe9de">
			<apply_if nightMode="true" attrColorValue="#544b4c"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="primaryRoadShadowColor">
		<case attrColorValue="#77464646">
			<apply_if nightMode="true" attrColorValue="#55ffffff"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="secondaryRoadColor">
		<case attrColorValue="#fff3de">
			<apply_if nightMode="true" attrColorValue="#544b4c"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="secondaryRoadLowZoomColor">
		<case attrColorValue="#fff3de">
			<apply_if nightMode="true" attrColorValue="#544b4c"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="secondaryRoadShadowColor">
		<case attrColorValue="#77464646">
			<apply_if nightMode="true" attrColorValue="#55ffffff"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="tertiaryRoadColor">
		<case attrColorValue="#ffffdc">
			<apply_if nightMode="true" attrColorValue="#57574a"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="tertiaryRoadLowZoomColor">
		<case attrColorValue="#ffffdc">
			<apply_if nightMode="true" attrColorValue="#57574a"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="tertiaryRoadShadowColor">
		<case attrColorValue="#969696">
			<apply_if nightMode="true" attrColorValue="#55ffffff"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="residentialRoadColor">
		<case attrColorValue="#ffffff">
			<apply_if nightMode="true" attrColorValue="#454545"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="residentialRoadShadowColor">
		<case attrColorValue="#969696">
			<apply_if nightMode="true" attrColorValue="#55ffffff"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="primaryResidentialHighwayAreaColor">
		<case attrColorValue="#ffffff">
			<apply_if nightMode="true" attrColorValue="#454545"/>
		</case>
	</renderingAttribute>

	<renderingAttribute name="serviceRoadColor">
		<case attrColorValue="#ffffff">
			<apply_if nightMode="true" attrColorValue="#454545"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="serviceRoadShadowColor">
		<case attrColorValue="#969696">
			<apply_if nightMode="true" attrColorValue="#55ffffff"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="serviceDrivewayRoadColor">
		<case attrColorValue="#ffffff">
			<apply_if nightMode="true" attrColorValue="#454545"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="serviceDrivewayRoadShadowColor">
		<case attrColorValue="#969696">
			<apply_if nightMode="true" attrColorValue="#55ffffff"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="roadRoadColor">
		<case attrColorValue="#ffffff">
			<apply_if nightMode="true" attrColorValue="#454545"/>
		</case>
	</renderingAttribute>

	<renderingConstant name="black" value="#000000"/>
	<renderingConstant name="white" value="#ffffff"/>
	<renderingAttribute name="highwayMotorwayTextColor">
		<case attrColorValue="$black">
			<apply_if nightMode="true" attrColorValue="#aaaaaa"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="highwayTrunkTextColor">
		<case attrColorValue="$black">
			<apply_if nightMode="true" attrColorValue="#aaaaaa"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="highwayPrimaryTextColor">
		<case attrColorValue="$black">
			<apply_if nightMode="true" attrColorValue="#aaaaaa"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="highwaySecondaryTextColor">
		<case attrColorValue="$black">
			<apply_if nightMode="true" attrColorValue="#aaaaaa"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="highwayTertiaryTextColor">
		<case attrColorValue="$black">
			<apply_if nightMode="true" attrColorValue="#aaaaaa"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="highwayResidentialServiceTextColor">
		<case attrColorValue="$black">
			<apply_if nightMode="true" attrColorValue="#aaaaaa"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="pedestrianRoadShadowColor">
		<case attrColorValue="$black">
			<apply_if nightMode="true" attrColorValue="#aaaaaa"/>
		</case>
	</renderingAttribute>

	<renderingAttribute name="trackColor">
		<case attrColorValue="#988050">
			<apply_if nightMode="true" attrColorValue="#b87900"/>
		</case>
	</renderingAttribute>
	<renderingConstant name="trackStrokeWidth" value="0.5"/>
	<renderingAttribute name="cyclewayColor">
		<case attrColorValue="#0000ff">
			<apply_if nightMode="true" attrColorValue="#0000ff"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="footwayColor">
		<case attrColorValue="#fa5073">
			<apply_if nightMode="true" attrColorValue="#c63f5c"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="cyclewayColor">
		<case attrColorValue="#0000ff">
			<apply_if nightMode="true" attrColorValue="#39398e"/>
		</case>
	</renderingAttribute>
	<renderingConstant name="cyclewayStrokeWidth" value="0.7"/>
	<renderingAttribute name="pathColor">
		<case attrColorValue="#555555">
			<apply_if nightMode="true" attrColorValue="#787878"/>
		</case>
	</renderingAttribute>
	<renderingConstant name="pathStrokeWidth" value="0.9"/>
	<renderingAttribute name="bridlewayColor">
		<case attrColorValue="#339900">
			<apply_if nightMode="true" attrColorValue="#339900"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="bridlewayColor">
		<case attrColorValue="#339900">
			<apply_if nightMode="true" attrColorValue="#265c0c"/>
		</case>
	</renderingAttribute>

<!-- PISTE COLORS -->
	<renderingAttribute name="pisteDefaultColor">
		<switch engine_v1="true">
			<case nightMode="true" attrColorValue="#cc777777"/>
			<case attrColorValue="#88525252"/>
		</switch>
		<case nightMode="true" attrColorValue="#aa777777"/>
		<case attrColorValue="#66525252"/>
	</renderingAttribute>
	<renderingAttribute name="pisteAreaDefaultColor">
		<case nightMode="true" attrColorValue="#cc727272"/>
		<case attrColorValue="#449d9d9d"/>
	</renderingAttribute>

	<renderingAttribute name="pisteTextColor">
		<case nightMode="true" attrColorValue="#ffffff"/>
		<case attrColorValue="#ffffff"/>
	</renderingAttribute>

	<renderingAttribute name="pisteDifficultyNoviceColor">
		<switch engine_v1="true">
			<case nightMode="true" attrColorValue="#6669f148"/>
			<case attrColorValue="#9920d740"/>
		</switch>
		<case nightMode="true" attrColorValue="#3369f148"/>
		<case attrColorValue="#5520d740"/>
	</renderingAttribute>
	<renderingAttribute name="pisteAreaDifficultyNoviceColor">
		<case nightMode="true" attrColorValue="#3369f148"/>
		<case attrColorValue="#6620d740"/>
	</renderingAttribute>

	<renderingAttribute name="pisteDifficultyEasyColor">
		<switch engine_v1="true">
			<case nightMode="true" attrColorValue="#994c48ff"/>
			<case attrColorValue="#661f19d0"/>
		</switch>
		<case nightMode="true" attrColorValue="#554c48ff"/>
		<case attrColorValue="#441f19d0"/>
	</renderingAttribute>
	<renderingAttribute name="pisteAreaDifficultyEasyColor">
		<case nightMode="true" attrColorValue="#551f19d0"/>
		<case attrColorValue="#551f19d0"/>
	</renderingAttribute>

	<renderingAttribute name="pisteDifficultyIntermediateColor">
		<switch engine_v1="true">
			<case nightMode="true" attrColorValue="#99d01a19"/>
			<case attrColorValue="#66d01a19"/>
		</switch>
		<case nightMode="true" attrColorValue="#55d01a19"/>
		<case attrColorValue="#44d01a19"/>
	</renderingAttribute>
	<renderingAttribute name="pisteAreaDifficultyIntermediateColor">
		<case nightMode="true" attrColorValue="#55d01a19"/>
		<case attrColorValue="#55d01a19"/>
	</renderingAttribute>

	<renderingAttribute name="pisteDifficultyAdvancedColor">
		<switch engine_v1="true">
			<case nightMode="true" attrColorValue="#bb9d9572"/>
			<case attrColorValue="#aa63615b"/>
		</switch>
		<case nightMode="true" attrColorValue="#889d9572"/>
		<case attrColorValue="#7763615b"/>
	</renderingAttribute>
	<renderingAttribute name="pisteAreaDifficultyAdvancedColor">
		<case nightMode="true" attrColorValue="#669d9572"/>
		<case attrColorValue="#6663615b"/>
	</renderingAttribute>

	<renderingAttribute name="pisteDifficultyExpertColor">
		<switch engine_v1="true">
			<case nightMode="true" attrColorValue="#ccd1962a"/>
			<case attrColorValue="#aafcb533"/>
		</switch>
		<case nightMode="true" attrColorValue="#77d1962a"/>
		<case attrColorValue="#66fcb533"/>
	</renderingAttribute>
	<renderingAttribute name="pisteAreaDifficultyExpertColor">
		<case nightMode="true" attrColorValue="#88d1962a"/>
		<case attrColorValue="#88fcb533"/>
	</renderingAttribute>

	<renderingAttribute name="pisteDifficultyFreerideColor">
		<switch engine_v1="true">
			<case nightMode="true" attrColorValue="#ccceb300"/>
			<case attrColorValue="#ccffde00"/>
		</switch>
		<case nightMode="true" attrColorValue="#77ceb300"/>
		<case attrColorValue="#77ffde00"/>
	</renderingAttribute>
	<renderingAttribute name="pisteAreaDifficultyFreerideColor">
		<case nightMode="true" attrColorValue="#88ceb300"/>
		<case attrColorValue="#66ffde00"/>
	</renderingAttribute>
	<renderingAttribute name="pisteDifficultyExtremeColor">
		<switch engine_v1="true">
			<case nightMode="true" attrColorValue="#aaf95a00"/>
			<case attrColorValue="#aaf95a00"/>
		</switch>
		<case nightMode="true" attrColorValue="#66f95a00"/>
		<case attrColorValue="#66f95a00"/>
	</renderingAttribute>
	<renderingAttribute name="pisteAreaDifficultyExtremeColor">
		<case nightMode="true" attrColorValue="#77f95a00"/>
		<case attrColorValue="#66f95a00"/>
	</renderingAttribute>

	<renderingAttribute name="woodColor">
		<case attrColorValue="#afdcc9">
			<apply_if nightMode="true" attrColorValue="#001122"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="forestColor">
		<case attrColorValue="#afdcc9">
			<apply_if nightMode="true" attrColorValue="#001122"/>
		</case>
	</renderingAttribute>

	<renderingConstant name="waterwayColorDay" value="#a4deff"/>
	<renderingConstant name="waterwayColorNight" value="#20005f"/>
	<renderingConstant name="heathColorDay" value="#f5ffff"/>
	<renderingConstant name="farmColorDay" value="#f5ffff"/>
	<renderingConstant name="farmyardColorDay" value="#f5ffff"/>
	<renderingConstant name="grassColorDay" value="#f5ffff"/>
	<renderingConstant name="meadowColorDay" value="#f5ffff"/>
	<renderingConstant name="brownfieldColorDay" value="#f5ffff"/>
	<renderingConstant name="villageGreenColorDay" value="#f5ffff"/>
	<renderingConstant name="parkColorDay" value="#aaf5ffff"/>
	<renderingConstant name="gardenColorDay" value="#f5ffff"/>
	<renderingConstant name="leisureCommonColorDay" value="#f5ffff"/>

	<renderingConstant name="null" value="#00000000"/>


	<order>
		<!-- Winter layers -->
		<switch area="false" point="true" objectType="1" order="128">
			<case tag="piste:type" value="downhill"/>
			<case tag="piste:type" value="nordic"/>
			<case tag="piste:type" value="skitour"/>
			<case tag="piste:type" value="hike"/>
			<case tag="piste:type" value="sled"/>
			<case tag="piste:type" value="sleigh"/>
			<case tag="leisure" value="ski_resort"/>
		</switch>
		<switch cycle="true" point="false" objectType="3" order="5">
			<case tag="piste:type" value="downhill"/>
			<case tag="piste:type" value="skitour"/>
			<case tag="piste:type" value="hike"/>
			<case tag="piste:type" value="sled"/>
			<case tag="piste:type" value="sleigh"/>
			<case tag="piste:type" value="snow_park"/>
			<case tag="leisure" value="ski_resort"/>
		</switch>

		<switch objectType="2">
			<case tag="piste:type" value="nordic" order="60"/>
			<case tag="piste:type" value="downhill" order="60"/>
			<case tag="piste:type" value="skitour" order="60"/>
			<case tag="piste:type" value="hike" order="60"/>
			<case tag="piste:type" value="sled" order="60"/>
			<case tag="piste:type" value="sleigh" order="60"/>
			<case tag="piste:type" value="snow_park" order="60"/>
		</switch>
	</order>


	<text>
		<switch minzoom="14" textOrder="8">
			<case tag="piste:type" value="nordic" nameTag="piste:name" nameTag2="ref"/>
			<case tag="piste:type" value="nordic" nameTag="" nameTag2="ref"/>
			<case tag="piste:type" value="downhill" nameTag="piste:name" nameTag2="ref"/>
			<case tag="piste:type" value="downhill" nameTag="" nameTag2="ref"/>
			<case tag="piste:type" value="skitour" nameTag="piste:name" nameTag2="ref"/>
			<case tag="piste:type" value="skitour" nameTag="" nameTag2="ref"/>
			<case tag="piste:type" value="hike" nameTag="piste:name" nameTag2="ref"/>
			<case tag="piste:type" value="hike" nameTag="" nameTag2="ref"/>
			<case tag="piste:type" value="sled" nameTag="piste:name" nameTag2="ref"/>
			<case tag="piste:type" value="sled" nameTag="" nameTag2="ref"/>
			<case tag="piste:type" value="sleigh" nameTag="piste:name" nameTag2="ref"/>
			<case tag="piste:type" value="sleigh" nameTag="" nameTag2="ref"/>
			<case tag="piste:type" value="snow_park"/>
			<apply>
				<case maxzoom="14" textSize="12"/>
				<case maxzoom="15" textSize="13"/>
				<case maxzoom="16" textSize="14"/>
				<case maxzoom="17" textSize="16"/>
				<case minzoom="18" textSize="18"/>
			</apply>
			<apply textColor="$pisteTextColor" textBold="true" textHaloRadius="2" textHaloColor="$pisteDefaultColor" textOnPath="true" textWrapWidth="25"/>
			<apply_if engine_v1="true" textDy="4"/>
			<apply>
				<case additional="piste:difficulty=intermediate" textHaloColor="$pisteDifficultyIntermediateColor"/>
<!--				<switch>-->
					<case additional="piste:difficulty=easy" textHaloColor="$pisteDifficultyEasyColor"/>
					<case additional="piste:difficulty=advanced" textHaloColor="$pisteDifficultyAdvancedColor"/>
<!--					<apply_if nightMode="false" textColor="#ffffff"/>
				</switch>-->
				<case additional="piste:difficulty=novice" textHaloColor="$pisteDifficultyNoviceColor"/>
				<case additional="piste:difficulty=expert" textHaloColor="$pisteDifficultyExpertColor"/>
				<case additional="piste:difficulty=freeride" textHaloColor="$pisteDifficultyFreerideColor"/>
				<case additional="piste:difficulty=extreme" textHaloColor="$pisteDifficultyExtremeColor"/>
			</apply>
		</switch>
		<case minzoom="11" tag="leisure" value="ski_resort" textColor="#080e94" textBold="true" textHaloRadius="4" textHaloColor="#aab9bcff" textWrapWidth="25" textOrder="2">
			<apply>
				<case maxzoom="16" textSize="14"/>
				<case maxzoom="17" textSize="16"/>
				<case minzoom="18" textSize="18"/>
			</apply>
			<apply_if nightMode="true" textColor="#eff0ff" textHaloColor="#44656cf6"/>
		</case>
		<switch textSize="12" textColor="#0066ff" textHaloRadius="1" textHaloColor="#f5ffff">
			<case tag="amenity" value="ski_rental"/>
			<apply_if nightMode="true" textHaloRadius="2" textColor="#b9d5ff" textHaloColor="#88355777"/>
			<apply_if engine_v1="true" textDy="9"/>
		</switch>

		<switch minzoom="14" textColor="#ffffff" textHaloRadius="2" textBold="true" textHaloColor="#99000000" textOrder="10">
			<case tag="aerialway" value="cable_car"/>
			<case tag="aerialway" value="gondola"/>
			<case tag="aerialway" value="chair_lift"/>
			<case tag="aerialway" value="mixed_lift"/>
			<case tag="aerialway" value="drag_lift"/>
			<case tag="aerialway" value="t-bar"/>
			<case tag="aerialway" value="j-bar"/>
			<case tag="aerialway" value="platter"/>
			<case tag="aerialway" value="magic_carpet"/>
			<case tag="aerialway" value="rope_tow"/>
			<apply nameTag2="ref" textOnPath="true">
				<case maxzoom="14" textSize="12"/>
				<case maxzoom="15" textSize="13"/>
				<case maxzoom="16" textSize="14"/>
				<case maxzoom="17" textSize="16"/>
				<case minzoom="18" textSize="18"/>
				<apply_if nightMode="true" textColor="#ffffff" textHaloColor="#77222222"/>
			</apply>
		</switch>

	</text>


	<point>
		<switch minzoom="12" tag="aerialway" value="station">
			<case maxzoom="16" icon="railway_subway_station_small_black" shield="white_round_small_shield" iconVisibleSize="3" intersectionSizeFactor="0.3" iconOrder="2">
				<apply_if nightMode="true" icon="railway_subway_station_small_white" shield="darkgray_round_small_shield"/>
			</case>
			<case minzoom="17" icon="aerialway_station" shield="white_black_round_shield"/>
		</switch>

		<switch minzoom="16" iconOrder="10" iconVisibleSize="24" intersectionSizeFactor="0.7">
			<case tag="piste:type" value="downhill" icon="piste_type_downhill"/>
			<case tag="piste:type" value="nordic" icon="piste_type_nordic"/>
			<case tag="piste:type" value="skitour" icon="piste_type_skitour"/>
			<case tag="piste:type" value="hike" icon="piste_type_hike"/>
			<switch>
				<case tag="piste:type" value="sled"/>
				<case tag="piste:type" value="sleigh"/>
				<apply icon="piste_type_sled"/>
			</switch>
			<apply>
				<case additional="piste:difficulty=novice" icon_2="skimap_overlay_novice"/>
				<case additional="piste:difficulty=easy" icon_2="skimap_overlay_easy"/>
				<case additional="piste:difficulty=intermediate" icon_2="skimap_overlay_intermediate"/>
				<case additional="piste:difficulty=advanced" icon_2="skimap_overlay_advanced"/>
				<case additional="piste:difficulty=expert" icon_2="skimap_overlay_expert"/>
				<case additional="piste:difficulty=freeride" icon_2="skimap_overlay_freeride"/>
				<case additional="piste:difficulty=extreme" icon_2="skimap_overlay_extreme"/>
			</apply>
			<apply icon__1="skimap_white_black_round_shield">
				<apply_if nightMode="true" icon__1="skimap_gray_black_round_shield"/>
			</apply>
		</switch>
		<case minzoom="15" tag="amenity" value="ski_rental" icon="amenity_ski_rental" shield="white_blue_square_shield">
			<apply_if nightMode="true" shield="gray_blue_square_shield"/>
		</case>
	</point>


	<polygon>
		<!--Winter landscape-->
		<switch>
			<case minzoom="12" tag="natural" value="field"/>
			<apply_if minzoom="14" color_2="#f5ffff" strokeWidth_2=":1"/>
			<apply_if nightMode="true" color="#004333" color_2="#004333"/>
		</switch>
		<switch>
			<case minzoom="11" tag="natural" value="beach" shader="beach"/>
			<case minzoom="12" tag="natural" value="mud" shader="mud"/>
			<apply_if nightMode="true" shader="" color="#334433"/>
		</switch>
		<case minzoom="12" tag="natural" value="scrub" color="#8decb6" shader="">
			<apply_if nightMode="true" shader="" color="#002014"/>
		</case>

		<switch minzoom="6">
			<case tag="natural" value="wood"/>
			<case tag="landuse" value="forest"/>
			<apply color="$woodColor" shader=""/>
		</switch>
		<switch>
			<case tag="place" value="island"/>
			<case tag="natural" value="land"/>
			<case tag="natural" value="grassland"/>
			<apply color="#f5ffff">
				<apply_if nightMode="true" color="#002a2a"/>
			</apply>
		</switch>

		<switch minzoom="12">
			<case tag="piste:type" value="downhill"/>
			<case tag="piste:type" value="skitour"/>
			<case tag="piste:type" value="hike"/>
			<case tag="piste:type" value="sled"/>
			<case tag="piste:type" value="sleigh"/>
			<case tag="piste:type" value="snow_park">
				<apply color_2="$pisteDefaultColor">
					<case maxzoom="12" pathEffect_2="16_2_3_2" strokeWidth_2="1.5:1.5"/>
					<case maxzoom="14" pathEffect_2="22_4_5_4" strokeWidth_2="2.5:2.5"/>
					<case maxzoom="16" pathEffect_2="27_6_9_6" strokeWidth_2="3.5:3.5"/>
					<case minzoom="17" pathEffect_2="40_7_10_7" strokeWidth_2="4.5:4.5"/>
				</apply>
			</case>
			<apply color="$pisteAreaDefaultColor">
				<case additional="piste:difficulty=novice" color="$pisteAreaDifficultyNoviceColor" color_2="$pisteDifficultyNoviceColor"/>
				<case additional="piste:difficulty=easy" color="$pisteAreaDifficultyEasyColor" color_2="$pisteDifficultyEasyColor"/>
				<case additional="piste:difficulty=intermediate" color="$pisteAreaDifficultyIntermediateColor" color_2="$pisteDifficultyIntermediateColor"/>
				<case additional="piste:difficulty=advanced" color="$pisteAreaDifficultyAdvancedColor" color_2="$pisteDifficultyAdvancedColor"/>
				<case additional="piste:difficulty=expert" color="$pisteAreaDifficultyExpertColor" color_2="$pisteDifficultyExpertColor"/>
				<case additional="piste:difficulty=freeride" color="$pisteAreaDifficultyFreerideColor" color_2="$pisteDifficultyFreerideColor"/>
				<case additional="piste:difficulty=extreme" color="$pisteAreaDifficultyExtremeColor" color_2="$pisteDifficultyExtremeColor"/>
			</apply>
		</switch>

		<case minzoom="11" tag="leisure" value="ski_resort" color="$null" strokeWidth_2="1:1" color_2="#99464fff" iconOrder="10">
			<apply_if nightMode="true" color_2="#99d0d2ff"/>
		</case>

	</polygon>


	<line>

		<switch minzoom="11">
			<switch>
				<case tag="piste:type" value="downhill"/>
				<case tag="piste:type" value="skitour">
					<apply>
						<case maxzoom="11" pathEffect="4_4"/>
						<case maxzoom="14" pathEffect="7_7"/>
						<case maxzoom="16" pathEffect="10_10"/>
						<case minzoom="17" pathEffect="13_13"/>
					</apply>
				</case>
				<apply>
					<case maxzoom="11" strokeWidth="2:2"/>
					<case maxzoom="13" strokeWidth="3:3"/>
					<case maxzoom="14" strokeWidth="4:4"/>
					<case maxzoom="15" strokeWidth="5.5:5.5"/>
					<case maxzoom="16" strokeWidth="7:7"/>
					<case minzoom="17" strokeWidth="10:10"/>
				</apply>
			</switch>
			<switch>
				<case tag="piste:type" value="nordic"/>
				<case tag="piste:type" value="hike">
					<apply>
						<case maxzoom="12" pathEffect="4_1"/>
						<case maxzoom="14" pathEffect="6_3"/>
						<case maxzoom="16" pathEffect="9_4"/>
						<case minzoom="17" pathEffect="12_5"/>
					</apply>
				</case>
				<apply>
					<case maxzoom="11" strokeWidth="0.6"/>
					<case maxzoom="13" strokeWidth="1:1"/>
					<case maxzoom="14" strokeWidth="2.5:2.5"/>
					<case maxzoom="16" strokeWidth="3.5:3.5"/>
					<case minzoom="17" strokeWidth="4.5:4.5"/>
				</apply>
			</switch>
			<switch>
				<switch>
					<case tag="piste:type" value="sled"/>
					<case tag="piste:type" value="sleigh"/>
					<apply>
						<case maxzoom="12" pathEffect="14_1"/>
						<case maxzoom="14" pathEffect="19_2"/>
						<case maxzoom="16" pathEffect="22_3"/>
						<case minzoom="17" pathEffect="30_4"/>
					</apply>
				</switch>
				<case tag="piste:type" value="snow_park">
					<apply>
						<case maxzoom="12" pathEffect="16_2_3_2"/>
						<case maxzoom="14" pathEffect="22_4_5_4"/>
						<case maxzoom="16" pathEffect="27_6_9_6"/>
						<case minzoom="17" pathEffect="40_7_10_7"/>
					</apply>
				</case>
				<apply>
					<case maxzoom="12" strokeWidth="1:1"/>
					<case maxzoom="14" strokeWidth="2.5:2.5"/>
					<case maxzoom="16" strokeWidth="3.5:3.5"/>
					<case minzoom="17" strokeWidth="4.5:4.5"/>
				</apply>
			</switch>
			<case tag="piste:type" value="snow_park"/>
			<apply>
				<case additional="piste:oneway=yes">
					<case minzoom="14" maxzoom="14" pathIcon="arrow_triangle_black_small_nobg" pathIconStep="25">
						<apply_if nightMode="true" pathIcon="arrow_triangle_white_small_nobg"/>
					</case>
					<case minzoom="15" maxzoom="16" pathIcon="arrow_triangle_black_nobg" pathIconStep="40">
						<apply_if nightMode="true" pathIcon="arrow_triangle_white_nobg"/>
					</case>
					<case minzoom="17" pathIcon="arrow_triangle_black_big_nobg" pathIconStep="50">
						<apply_if nightMode="true" pathIcon="arrow_triangle_white_big_nobg"/>
					</case>
				</case>
				<case additional="piste:oneway=no">
					<case minzoom="14" maxzoom="14" pathIcon="skimap_arrow_2triangles_black_small_nobg" pathIconStep="40">
						<apply_if nightMode="true" pathIcon="skimap_arrow_2triangles_white_small_nobg"/>
					</case>
					<case minzoom="15" maxzoom="16" pathIcon="skimap_arrow_2triangles_black_nobg" pathIconStep="60">
						<apply_if nightMode="true" pathIcon="skimap_arrow_2triangles_white_nobg"/>
					</case>
					<case minzoom="17" pathIcon="skimap_arrow_2triangles_black_big_nobg" pathIconStep="80">
						<apply_if nightMode="true" pathIcon="skimap_arrow_2triangles_white_big_nobg"/>
					</case>
				</case>
			</apply>
			<apply_if additional="piste:abandoned=yes" cap="ROUND">
				<case maxzoom="12" pathEffect="1_17"/>
				<case maxzoom="14" pathEffect="1_30"/>
				<case maxzoom="16" pathEffect="1_45"/>
				<case maxzoom="18" pathEffect="1_65"/>
			</apply_if>
			<apply color="$pisteDefaultColor" color_2="#000000" color__2="#555555" color__1="#555555">
				<apply_if nightMode="true" color__2="#ffffff" color__1="#ffffff"/>
			</apply>
			<apply>
				<switch>
					<case additional="piste:difficulty=novice" color="$pisteDifficultyNoviceColor"/>
					<case additional="piste:difficulty=easy" color="$pisteDifficultyEasyColor"/>
					<case additional="piste:difficulty=intermediate" color="$pisteDifficultyIntermediateColor"/>
					<case additional="piste:difficulty=advanced" color="$pisteDifficultyAdvancedColor"/>
					<case additional="piste:difficulty=expert" color="$pisteDifficultyExpertColor"/>
					<case additional="piste:difficulty=freeride" color="$pisteDifficultyFreerideColor"/>
					<case additional="piste:difficulty=extreme" color="$pisteDifficultyExtremeColor"/>
				</switch>
			</apply>
			<apply minzoom="14" pisteGrooming="true">
				<switch>
					<case additional="piste:grooming=classic"/>
					<case additional="piste:grooming=mogul"/>
					<switch>
						<case additional="piste:grooming=skating" pathEffect__2="32_4" pathEffect__1="2_2_2_30" CAP__1="BUTT"/>
						<case additional="piste:grooming=classic+skating" pathEffect__1="2_30" CAP__1="BUTT"/>
						<apply>
							<case maxzoom="14" strokeWidth__1="3:3"/>
							<case minzoom="15" strokeWidth__1="4:4"/>
						</apply>
					</switch>
					<case additional="piste:grooming=scooter" pathEffect__2="17_4"/>
					<case additional="piste:grooming=backcountry" pathEffect__2="5_20"/>
					<apply>
						<case maxzoom="14" strokeWidth__2="1:1"/>
						<case minzoom="15" strokeWidth__2="2:2"/>
					</apply>
				</switch>
			</apply>
		</switch>

		<switch minzoom="12">
			<switch>
				<case tag="aerialway" value="gondola"/>
				<case tag="aerialway" value="cable_car"/>
				<apply>
					<case maxzoom="14" strokeWidth="1:1"/>
					<case maxzoom="16" strokeWidth="2:2"/>
					<case minzoom="17" strokeWidth="3:3"/>
				</apply>
			</switch>
			<switch>
				<switch>
					<case tag="aerialway" value="chair_lift"/>
					<case tag="aerialway" value="t-bar"/>
					<case tag="aerialway" value="j-bar"/>
					<case tag="aerialway" value="platter"/>
					<case tag="aerialway" value="magic_carpet"/>
					<case tag="aerialway" value="rope_tow"/>
					<apply minzoom="14" pathIcon="arrow_triangle_white2">
						<apply_if nightMode="true" pathIcon="arrow_triangle_black2"/>
					</apply>
				</switch>
				<case tag="aerialway" value="mixed_lift"/>
				<apply>
					<case maxzoom="14" strokeWidth="0.6"/>
					<case maxzoom="16" strokeWidth="1:1"/>
					<case minzoom="17" strokeWidth="1.5:1.5"/>
				</apply>
			</switch>
			<case tag="aerialway" value="drag_lift">
				<case maxzoom="14" strokeWidth="0.5"/>
				<case maxzoom="16" strokeWidth="0.8"/>
				<case minzoom="17" strokeWidth="1:1"/>
			</case>
			<apply>
				<switch minzoom="14">
					<case additional="oneway=no" pathIcon="arrow_2triangles_white">
						<apply_if nightMode="true" pathIcon="arrow_2triangles_black"/>
					</case>
					<case additional="oneway=yes" pathIcon="arrow_triangle_white2">
						<apply_if nightMode="true" pathIcon="arrow_triangle_black2"/>
					</case>
				</switch>
			</apply>
			<apply>
				<case maxzoom="15" pathIconStep="30"/>
				<case maxzoom="16" pathIconStep="35"/>
				<case minzoom="17" pathIconStep="45"/>
			</apply>
			<apply color="#000000">
				<apply_if nightMode="true" color="#ffffff"/>
			</apply>
		</switch>
		<switch engine_v1="false" minzoom="14" strokeWidth="0.3">
			 <case tag="natural" value="cliff" color="#148c14" pathIcon="stroke_darkgreen" pathIconStep="10"/>
			 <case tag="natural" value="slope" color="#88949494" pathIcon="stroke_gray" pathIconStep="10"/>
		</switch>


	</line>
</renderingStyle>