	<renderingConstant name="cyclewayPathEffect" value="12_4"/>
	<renderingConstant name="footwayColorDay" value="#0033ff"/>
	<renderingConstant name="footwayColorNight" value="#c79f00"/>
	<renderingConstant name="footwayStrokeWidth" value="1"/>
	<renderingConstant name="pathColorDay" value="#00896e"/>
