	<renderingConstant name="motorwayRoadDesaturatedColorDay" value="#bcc6ec"/>
	<renderingConstant name="motorwayRoadDesaturatedColorNight" value="#3f4764"/>
	<renderingConstant name="trunkRoadDesaturatedColorDay" value="#ffc7d0"/>
