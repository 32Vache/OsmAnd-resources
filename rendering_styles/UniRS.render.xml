	<renderingConstant name="accessCustomersColorNight" value="#b671be"/>
	<renderingConstant name="accessDeliveryColor" value="#9383a5"/>
	<renderingConstant name="accessDeliveryColorNight" value="#9383a5"/>-->
	<renderingConstant name="pedestrianModePavingStonesShader" value="paving_stones_green"/>
	<renderingConstant name="pedestrianModePavingStonesShaderNight" value="paving_stones_green_night"/>

	<renderingConstant name="contourDayColorNon100m" value="#33ff7c00"/>
	<renderingConstant name="contourDayColor100m" value="#33ea7300"/>
	<renderingConstant name="contourNightColorNon100m" value="#2a94775c"/>
	<renderingConstant name="contourNightColor100m" value="#2a94775c"/>

	<!-- Forest -->
	<renderingConstant name="woodColorDay" value="#9cd77b"/>
	<renderingConstant name="forestColorDay" value="#9cd77b"/>
	<renderingConstant name="woodColorDesaturatedDay" value="#b4d0a9"/>
	<renderingConstant name="forestColorDesaturatedDay" value="#aac599"/>
	<renderingConstant name="woodColorNight" value="#134025"/>
	<renderingConstant name="woodBroadleavedShader" value="wood_broadleaved2"/>
	<renderingConstant name="woodNeedleleavedShader" value="wood_needleleaved2"/>
	<renderingConstant name="woodMixedShader" value="wood_mixed2"/>

	<renderingConstant name="waterwayColorDay" value="#41cad3"/>
	<renderingConstant name="waterwayColorNight" value="#330099"/>
	<renderingConstant name="waterwayDesaturatedColorDay" value="#b5d0d0"/>
	<renderingConstant name="waterwayDesaturatedColorNight" value="#184d6f"/>
	<renderingConstant name="waterwayTunnelColorNight" value="#444a48aa"/>
	<renderingConstant name="waterwayTunnelDashColorDay" value="#aaffffff"/>
	<renderingConstant name="waterwayTunnelDashColorNight" value="#447340d9"/>
<!--
	<renderingConstant name="parkingColorNight" value="#41536c"/>
	<renderingConstant name="bicycleParkingColorNight" value="#41536c"/>-->

	<renderingConstant name="parkingPrivateColorDay" value="#77f8f5df"/>
	<renderingConstant name="parkingPrivateColorNight" value="#7741536c"/>
	<renderingConstant name="parkingPermissiveColorDay" value="#2251ff53"/>
	<renderingConstant name="parkingPermissiveColorNight" value="#1151ff53"/>
	<renderingConstant name="parkingCustomersColorDay" value="#33a247ad"/>
	<renderingConstant name="parkingCustomersColorNight" value="#44a247ad"/>
	<renderingConstant name="parkColorDay" value="#b0b6fdb6"/>
	<renderingConstant name="parkColorDayStroke" value="#9db487"/>
	<renderingConstant name="landuseIndustrialColorNight" value="#19283d"/>
	<renderingConstant name="landuseResidentialColorNight" value="#443f41"/>
	<renderingConstant name="landuseRailwayColorNight" value="#19283d"/>
	<renderingConstant name="landusePrisonColorNight" value="#19283d"/>
	<renderingConstant name="landuseCommercialColorNight" value="#43242d"/>
	<renderingConstant name="landuseLandfillColorNight" value="#505501"/>
	<renderingConstant name="amenityEducationalColorNight" value="#372c29"/>
	<renderingConstant name="amenityEducationalStrokeColorNight" value="#71554e"/>
	<renderingConstant name="amenityHospitalColorNight" value="#372c29"/>
	<renderingConstant name="landuseAllotmentsColorNight" value="#2c5100"/>
	<renderingConstant name="landuseGaragesColorNight" value="#3b3839"/>
	<renderingConstant name="leisureResortColorNight" value="#b0004333"/>
	<renderingConstant name="gardenColorNight" value="#2c5100"/>
	<renderingConstant name="farmColorNight" value="#232d01"/>
	<renderingConstant name="farmyardColorNight" value="#3d3528"/>
	<renderingConstant name="wetlandColorNight" value="#304a5e"/>
	<renderingConstant name="meadowColorNight" value="#232d01"/>

	<order>

		<group objectType="2" area="false" point="false">
			<filter appMode="car" layer="-1" tag="public_transport" value="station" order="10"/>

			<filter tag="power" value="line" order="50"/>
			<filter tag="power" value="minor_line" order="50"/>
		</group>

	</order>


	<text>

	<!-- Added support of fixme, note, description and inscription tags -->
<!--		<group showNotes="true">
			<filter tag="fixme" value=""/>
			<filter tag="note" value=""/>
			<filter tag="description" value=""/>
			<filter tag="inscription" value=""/>
			<groupFilter textSize="10" minzoom="16" textColor="#000000" textHaloRadius="1" textDy="7"/>
		</group> -->

		<group textColor="#000000" textWrapWidth="20" textHaloRadius="$textHaloRadiusDay">
			<filter minzoom="$placeLocalityMinZoom" textSize="11" tag="place" value="locality" textColor="$placeSuburbLocalityTextColorDay" textOrder="15"/>
			<groupFilter nightMode="true" textColor="#ffffff" textHaloRadius="$textHaloRadiusNight" textHaloColor="#77777777"/>
		</group>

		<group textColor="#000000" textDy="-2">
			<group textHaloRadius="$textHaloRadiusDay" textHaloColor="#ffffff">
				<filter tag="highway" value="pedestrian" textOrder="11"/>
				<filter tag="highway" value="footway" textOrder="12"/>
				<groupFilter nightMode="true" textColor="#ffffff" textHaloRadius="$textHaloRadiusNight" textHaloColor="#77777777"/>
				<groupFilter appMode="pedestrian" textHaloColor="$pedestrianRoadModeColorDay">
					<filter nightMode="true" textHaloColor="$pedestrianRoadModeColorNight"/>
				</groupFilter>
			</group>
			<groupFilter textOnPath="true" nameTag="">
				<filter minzoom="14" maxzoom="14" textSize="12"/>
				<filter minzoom="15" maxzoom="15" textSize="13"/>
				<filter minzoom="16" maxzoom="16" textSize="14"/>
				<filter minzoom="17" maxzoom="17" textSize="16"/>
				<filter minzoom="18" textSize="18"/>
			</groupFilter>
		</group>

	</text>


	<point>
		<!-- Added support of fixme, note, description, inscription tags -->
	<!--    <group showNotes="true">
		<filter icon="natural_volcano" tag="fixme" value=""/>
			<filter icon="tourism_information" tag="note" value=""/>
			<filter icon="tourism_information" tag="description" value=""/>
			<filter icon="tourism_information" tag="inscription" value=""/>
			<groupFilter minzoom="14"/>
		</group>    -->

	</point>


	<polygon>

		<group>   <!-- Buildings and landuse+amenities group -->
			<group>    <!-- Buildings group-->
				<group>
					<filter minzoom="13" color="#bbbbbb" tag="power" value="generator"/>
					<filter minzoom="13" color="#bbbbbb" tag="power" value="plant"/>
					<filter minzoom="14" color="#bbbbbb" tag="power" value="substation"/>
					<groupFilter nightMode="true" color="#132c4e"/>
				</group>

				<group minzoom="16">
					<filter tag="building" value="church"/>
					<filter tag="building" value="cathedral"/>
					<filter tag="building" value="chapel"/>
					<filter tag="building" value="mosque"/>
					<groupFilter strokeWidth_2="1.3" color="#b6a0b5" color_2="#920029"/>
					<groupFilter nightMode="true" color="#795877" color_2="#6dffd6"/>
				</group>

				<!-- Added support of abandoned and ruins tags -->
				<groupFilter additional="ruins=yes" color_2="#999999" strokeWidth_2="0.5" pathEffect_2="9_3">
					<filter nightMode="true" color_2="#aaaaaa"/>
				</groupFilter>
				<groupFilter additional="abandoned=yes" color_2="#000000" strokeWidth_2="0.5" pathEffect_2="9_3">
					<filter nightMode="true" color_2="#aaaaaa"/>
				</groupFilter>

			</group>  <!-- /Buildings -->

		</group>   <!-- /Buildings and landuse+amenities -->
		
		<group noPolygons="false">

			<group minzoom="13" color="#f2caea" color_2="#920029">
				<filter tag="tourism" value="attraction"/>
				<filter tag="amenity" value="monastery"/>
				<filter tag="historic" value="monastery"/>
				<groupFilter subwayMode="false" strokeWidth_2="1.3"/>
				<groupFilter nightMode="true" color="#834522" color_2="#6dffd6"/>
			</group>

			<group>
				<filter minzoom="16" color="#66f2caea" color_2="#734a08" strokeWidth_2=":2" pathEffect_2="9_3" tag="tourism" value="theme_park"/>
				<groupFilter nightMode="true" color="#776f4664" color_2="#71b7b5"/>
			</group>
			<filter nightMode="true" minzoom="15" tag="leisure" value="stadium" color="#04382c"/>
			<filter nightMode="true" minzoom="15" tag="leisure" value="sports_centre" color="#0b4336"/>
			<filter nightMode="true" minzoom="8" tag="leisure" value="park" color="#035340"/>

			<!-- Added support of disused,abandoned,ruins tags to polygonal place_of_worship amenity -->
			<filter minzoom="15" tag="amenity" value="place_of_worship" color="#f2caea" color_2="#920029" strokeWidth_2="1.7">
				<filter additional="disused=yes" color_2="#920029" pathEffect_2="9_3"/>
				<filter additional="abandoned=yes" color_2="#920029" pathEffect_2="9_3"/>
				<filter additional="ruins=yes" color_2="#920029" pathEffect_2="9_3"/>
				<groupFilter nightMode="true" color="#834522" color_2="#6dffd6"/>
			</filter>

			<!-- Changing waterway colors for better visibility in the forest-->
			<group>
				<filter tag="waterway" value="riverbank"/>
				<filter tag="waterway" value="canal"/>
				<filter tag="waterway" value="dock"/>
				<filter tag="waterway" value="mill_pond"/>
				<groupFilter color="$waterwayColorDay"/>
				<groupFilter subwayMode="true" color="$waterwayDesaturatedColorDay"/>
				<groupFilter nightMode="true" color="$waterwayColorNight"/>
				<groupFilter nightMode="true" subwayMode="true" color="$waterwayDesaturatedColorNight"/>
			</group>
			<group>
				<filter minzoom="1" tag="natural" value="coastline" color_2="#2Aff7c00" strokeWidth_2="1"/>
				<filter minzoom="4" tag="natural" value="water"/>
				<filter minzoom="4" tag="natural" value="lake"/>
				<filter minzoom="4" tag="natural" value="bay"/>
				<filter minzoom="7" tag="landuse" value="reservoir"/>
				<filter minzoom="7" tag="landuse" value="water"/>
				<filter minzoom="10" tag="landuse" value="basin"/>
				<filter minzoom="15" tag="leisure" value="swimming_pool" color_2="#7479ec" strokeWidth_2="1"/>
				<groupFilter color="$waterwayColorDay"/>
				<groupFilter subwayMode="true" color="$waterwayDesaturatedColorDay"/>
				<groupFilter nightMode="true" color="$waterwayColorNight"/>
				<groupFilter nightMode="true" subwayMode="true" color="$waterwayDesaturatedColorNight"/>
			</group>

		</group>

		<filter minzoom="13" tag="aeroway" value="apron" color="#e0c4fb">
			<filter nightMode="true" color="#4c3a5d"/>
		</filter>

		<filter minzoom="14" tag="aeroway" value="terminal" color="#cc99ff" strokeWidth_2="0.3" color_2="#000000">
			<filter nightMode="true" color="#a65ae1" color_2="#999999"/>
		</filter>

		<filter appMode="car" layer="-1" tag="public_transport" value="platform" disable="true"/>
		<filter appMode="car" layer="-1" tag="railway" value="platform" disable="true"/>

	</polygon>


	<line>

		<switch>
			<case tag="cycleway" value="lane"/>
			<case tag="cycleway" value="track"/>
			<case tag="cycleway" value="opposite_lane"/>
			<case tag="cycleway" value="share_busway"/>
			<case tag="cycleway" value="opposite"/>
			<groupFilter color="#a85bff" pathEffect="12_4">
				<filter appMode="bicycle" minzoom="12" maxzoom="13" strokeWidth="3"/>
				<filter appMode="bicycle" minzoom="14" strokeWidth="4"/>
				<filter appMode="car" minzoom="13" maxzoom="16" disable="true"/>
				<filter appMode="car" color="#15a85bff" strokeWidth="1.5"/>
<!--				<filter lessDetailed="true" minzoom="12" maxzoom="12" disable="true"/>-->
				<filter minzoom="12" maxzoom="12" strokeWidth="1"/>
				<filter minzoom="13" maxzoom="13" strokeWidth="1.5"/>
				<filter minzoom="14" strokeWidth="1.5"/>
			</groupFilter>
		</switch>

		<group layer="-1" subwayMode="false" minzoom="13" pathEffect="5_3">
			<filter appMode="car" tag="railway" value="subway" disable="true"/>
			<filter appMode="car" tag="railway" value="light_rail" disable="true"/>
			<filter appMode="car" tag="railway" value="construction" disable="true"/>
			<filter nightMode="true" tag="railway" value="subway" color="#666666" strokeWidth="2"/>
			<filter nightMode="true" tag="railway" value="light_rail" color="#505050" strokeWidth="2"/>
			<filter appMode="bicycle" tag="railway" value="subway" color="#25999999" strokeWidth="2"/>
			<filter appMode="bicycle" tag="railway" value="light_rail" color="#25666666" strokeWidth="1:1"/>
		</group>

	</line>
</renderingStyle>