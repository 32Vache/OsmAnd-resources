<renderingStyle name="UniRS" depends="default" defaultColor="#f1eae4" version="1.2">
<!-- Universal Rendering Style for OsmAnd by xmd5a (xmd5a@yandex.ru) 11.12.2013 http://forum.openstreetmap.org/viewtopic.php?id=23125 -->

	<renderingAttribute name="shadowRendering">
	<!-- 0 - no shadow, 1 - one step, 2 - blur shadow, 3 - solid shadow -->
		<!-- next line ensures bridges stand out ok -->
		<case minzoom="14" nightMode="false" attrIntValue="3" shadowColor="#bb444444"/>
<!--		<case minzoom="14" attrIntValue="3" shadowColor="#464646"/>-->
		<case minzoom="9" maxzoom="13" attrIntValue="3" shadowColor="#969696"/>
		<case attrIntValue="0"/>
	</renderingAttribute>

	<renderingAttribute name="route">
		<filter nightMode="false" color="#ccf033ca" strokeWidth="7:6"/>
		<filter color="#77ff00c0" strokeWidth="7:6"/>
	</renderingAttribute>
	<renderingAttribute name="gpx">
		<filter strokeWidth="7:6" pathEffect="6_5" cap="BUTT">
			<filter nightMode="true" color="#ccf40977"/>
			<filter additional="routePoints=true" pathEffect="4_3" color="#bbff6600"/>
			<filter color="#bbff6600"/>
		</filter>
	</renderingAttribute>

