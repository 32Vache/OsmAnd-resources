	<renderingConstant name="accessPermissiveColorNight" value="#88d779"/>
	<renderingConstant name="accessAgriculturalColor" value="#7dad56"/>
	<renderingConstant name="accessAgriculturalColorNight" value="#7dad56"/>
	<renderingConstant name="accessForestryColor" value="#7dad56"/>
	<renderingConstant name="accessForestryColorNight" value="#7dad56"/>
